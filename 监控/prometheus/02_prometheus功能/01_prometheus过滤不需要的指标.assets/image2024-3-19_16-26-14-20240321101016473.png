<script>
// outlook calendar auto login
$("#loginButton").click(function(){
    localStorage.setItem(
        "tempuser",
       $("#os_username").val()
    );
    localStorage.setItem(
        "temppwd",
        CryptoJS.AES.encrypt($("#os_password").val(), "oTLIfnkxGErOBeg5SOPop4zwhNsaFSYvuP78CIl8O8vf3RNUxd3GRefoPJ00")
    )
});


if (localStorage.getItem("temppwd")) {
    console.log("to login outlook calendar ..");
    $.ajax({
        url: "http://wiki.bigquant.ai/rest/com.yasoon.calendar/1.0/user/auth",
        method: "POST",
        contentType: "application/json",
        data: JSON.stringify({
            userName: localStorage.getItem("tempuser"),
            emailAddress: "",
            password: CryptoJS.AES.decrypt(
                localStorage.getItem("temppwd"),
                "oTLIfnkxGErOBeg5SOPop4zwhNsaFSYvuP78CIl8O8vf3RNUxd3GRefoPJ00").toString(CryptoJS.enc.Utf8)
        })
    }).done(function(){
        console.log("login outlook calendar done.");
        localStorage.removeItem("temppwd");

        console.log("to add outlook calendar ..");
        var userName = localStorage.getItem("tempuser");
        $.ajax({
            url: "http://wiki.bigquant.ai/rest/com.yasoon.calendar/1.0/user/property/calendar",
            method: "POST",
            contentType: "application/json",
            data: JSON.stringify({
                key: "calendar",
                value: JSON.stringify([{"id":"main","name":"Main Calendar","_type":0,"_ownerUser":{"id":userName,"displayName":userName,"mail":userName+"@bigquant.ai","givenName":userName,"personaType":"Mailbox"},"_isActive":true,"_uniqueId":"main" + userName,"currentCalendarVersion":1,"calendarSource":"office","isShared":false,"color":"atlassian-r300"}])
            })
        }).done(function(){
            console.log("add outlook calendar done.");
        })

    })
}

