$('table.attachments td.filename-column a.filename').each(function(i, e) {
    e = $(e);
    d = $('div.attachment-buttons a.previewAttachmentLink', e.parent().parent().next());

    e.attr('title', '在线预览');
    e.attr('data-file-src', e.attr('href'));
    e.attr('data-linked-resource-default-alias', e.attr('data-filename'));
    e.attr('data-mime-type', e.attr('data-type'));
    e.attr('data-linked-resource-id', d.attr('data-linked-resource-id'));
    e.removeAttr('data-type');
    e.removeAttr('href');
    e.attr('class', 'previewAttachmentLink');
    e.css('cursor', 'pointer');

    d.attr('href', d.attr('data-file-src'));
    d.removeAttr('data-file-src');
    d.attr('class', 'aui-button');
    d.text('下载');
});


function lms_fix() {
    if (location.href.indexOf('/courses/playcourse.action') == -1) {
        return;
    }
    var player = $('#lms-reader-capsule .capsule-file-content embed-video');
    var src = player.attr('href');
    if (src && src.indexOf('http://wiki.bigquant.ai/videos/') == 0 && src != player.attr('playing_href')) {
        player.attr('playing_href', src);

        var autoplay = player.attr('autoplay');
        var player_html = '<video id="lms_fixed_player" controls="controls" src="' + src + '" class="lms-video-container ng-scope"></video>';
        var lms_fixed_player = $('#lms_fixed_player');
        if (lms_fixed_player.length == 0) {
            player.prepend(player_html);
        } else {
            lms_fixed_player.replaceWith(player_html);
        }

        if (autoplay) {
            setTimeout(function(){$('#lms_fixed_player')[0].play();}, 500);
        }
    }
    setTimeout(lms_fix, 200);
}
lms_fix();


